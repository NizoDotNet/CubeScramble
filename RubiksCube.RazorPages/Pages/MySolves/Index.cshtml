@page "{handler?}"
@model RubiksCube.RazorPages.Pages.MySolves.IndexModel

<div class="container">
    <div class="Scrambles ">
        @foreach(var i in Model.Scrambles)
        {
            <li>
                @i.Time
                <a class="btn btn-danger" asp-route-id="@i.Id" asp-page="/MySolves/Delete">Delete</a>
            </li>
        }
        <img />
    </div>
</div>